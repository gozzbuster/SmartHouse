<?xml version="1.0" encoding="utf-8" ?>
<VoiceCommands xmlns="http://schemas.microsoft.com/voicecommands/1.2">
  <CommandSet xml:lang="en-us" Name="AdventureWorksCommandSet_en-us">
    <CommandPrefix> In my project </CommandPrefix>
    <Example> Switch the light </Example>

    <Command Name="lightOn">
      <Example>Turn on a light </Example>
      <ListenFor>Turn on the light in the {room} </ListenFor>
      <Feedback>Turning on the light in the {room} </Feedback>
      <Navigate />
    </Command>
    <Command Name="lightOnByTime">
      <Example>Turn on a light </Example>
      <ListenFor>Turn on the light in the {room} at {hours}:{minutes} </ListenFor>
      <Feedback>the light will switch in the {room} at {hours} {minutes} </Feedback>
      <Navigate />
    </Command>
    <Command Name="lightOff">
      <Example> turn off a light </Example>
      <ListenFor> turn off the light in the {room} </ListenFor>
      <Feedback> turning off the light in the {room} </Feedback>
      <Navigate />
    </Command>
    <Command Name="lightOffByTime">
      <Example> turn off a light </Example>
      <ListenFor> turn off the light in the {room} at {hours}:{minutes} </ListenFor>
      <Feedback> OK the light will switch in the {room} at {hours} {minutes} </Feedback>
      <Navigate />
    </Command>
    <PhraseList Label="room">
    <Item>Bedroom</Item>
    <Item>Kitchen</Item>
    <Item>Living room</Item>
    <Item>Corridor</Item>
    <Item>Hall</Item>
